{
  "name": "mix",
  "version": "1.0.0",
  "description": "Front-end Template",
  "main": "index.js",
  "scripts": {
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "production": "mix --production",
    "tailwind-config-viewer": "tailwind-config-viewer -o --port 8081",
    "serve": "concurrently --kill-others \"npm run watch\" \"npm run tailwind-config-viewer\""
  },
